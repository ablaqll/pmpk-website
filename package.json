{
  "name": "pmpk-website",
  "version": "1.0.0",
  "description": "PMPK №9 Official Website - Psychological-Medical-Pedagogical Consultation, Astana, Kazakhstan",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "server": "tsx watch server/index.ts",
    "dev:all": "concurrently \"npm run server\" \"npm run dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "start": "NODE_ENV=production tsx server/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx server/seed.ts",
    "db:reset": "rm -f sqlite.db && tsx server/seed.ts",
    "railway:deploy": "railway up",
    "railway:seed": "railway run npm run db:seed",
    "railway:logs": "railway logs --follow",
    "railway:open": "railway open",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx"
  },
